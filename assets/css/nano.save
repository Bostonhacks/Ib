  @media screen and (max-width: 650px) {
    .column {
      width: 100%;
      display: block;
    }
  }
  
  body
  {
    background-color: #eff0f2;
    font-family: 'Roboto', sans-serif;
  }
  
  .organizer-container {
    padding: 0 16px;
    display:flex;
  align-items:center;
  flex-direction: row;
  }
  
  .organizer-container::after, .row::after {
    content: "";
    clear: both;
    display: table;
  }
  
  .title {
    color: grey;
    font-size: 95%;
  }
  table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
  }
  
  td, th {
    text-align: left;
    padding: 8px;
  }

  tr:nth-child(even) {
    background-color: #dddddd;
  }


